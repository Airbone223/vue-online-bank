/router-link>\r\n   </td>\r\n   <td>{{req.id}}</td>\r\n </tr>\r\n    </tbody>\r\n  </table>\r\n</template>\r\n\r\n<script>\r\nimport {currency} from '@/utils/currency-formator'\r\nimport AppStatus from '@/components/ui/AppStatus'\r\n\r\nexport default {\r\nprops: ['requests'],\r\n  setup() {\r\n  return {currency}\r\n  },\r\n  components: {AppStatus}\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"],"sourceRoot":""}]}