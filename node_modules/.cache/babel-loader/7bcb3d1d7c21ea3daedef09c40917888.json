/create', values)\r\n    emit('created')\r\n  }\r\n  return {\r\n    ...useRequestForm(submit)\r\n  }\r\n}\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"],"sourceRoot":""}]}